
<div class="page-header">
  <h1> Sprints <small></small></h1>
</div>

<%= form_for([@project, @project.issue.build]) do |f| %>
  <p>
    <%= f.label :issue_type %><br>
    <%= f.text_field :issue_type %>
  </p>
  <p>
    <%= f.label :issue_description %><br>
