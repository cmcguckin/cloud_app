<div class="page-header">
  <h1> <%= @projects.name %> <small> <%= @projects.description %></small></h1>
</div>
<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">
  Project <%= @projects.name %> 
  </div>
  <div class="panel-body">
  <p>  Text here
  </p>
<p>
  <strong>Name:</strong>
  <%= @projects.name %>
</p>
<p>
  <strong>Description:</strong>
  <%= @projects.description %>
</p>
<p>
  <strong>Objective:</strong>
  <%= @projects.objectives %>
</p>
<p>
  <strong>Benefits</strong>
  <%= @projects.benefits%>
</p>
<p>
  <strong>Data Required</strong>
  <%= @projects.data_required %>
</p> 

<%= link_to 'Edit', edit_project_path(@projects) %> |
<%= link_to 'Delete', project_path(@projects),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
</div>
<div class="panel-body">
  <%= render @projects.issues %>
</div>


<div class="panel-heading"> Sprints </div>
<div class="panel-body">
  <%= render 'issues/form' %>
</div>

</div>